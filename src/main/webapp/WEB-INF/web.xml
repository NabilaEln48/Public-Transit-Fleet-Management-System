<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
   version="4.0">
    
    <servlet>
        <servlet-name>FrontControllerServlet</servlet-name>
        <servlet-class>com.algonquincollege.simran_final_project.controller.FrontControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FrontControllerServlet</servlet-name>
        <url-pattern>/FrontController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddFuelLogServlet</servlet-name>
        <servlet-class>com.algonquincollege.simran_final_project.controller.AddFuelLogServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFuelLogServlet</servlet-name>
        <url-pattern>/addFuelLog</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewFuelLogsServlet</servlet-name>
        <servlet-class>com.algonquincollege.simran_final_project.controller.ViewLogsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewFuelLogsServlet</servlet-name>
        <url-pattern>/ViewFuelLogs</url-pattern>
    </servlet-mapping>
    
</web-app>
